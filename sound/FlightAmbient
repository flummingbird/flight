//FlightAmbient by WWC
//Ambient sounds for air
//noise+filters for wind

Noise wind => Gain g => ResonZ wind_filter => JCRev r => dac;

//Global Param
0.05 => r.mix;
0.1 => g.gain;
.001 => float t;

//1000-9000hz
wind_filter.set(3000, 3);




//time loop
while( true ) {
    
    
        100+Std.fabs(Math.sin(t) * 9000) => wind_filter.freq;
       t + 0.001 =>t;
        <<<t>>>;
        10::ms => now;
    }
